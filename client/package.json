{
  "name": "todoapp",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "^1.0.2",
    "css-loader": "^0.19.0",
    "style-loader": "^0.12.1"
  },
  "dependencies": {
    "axios": "0.13.1",
    "bootstrap": "^3.3.6",
    "classnames": "^2.2.5",
    "immutable": "^3.7.5",
    "jsonwebtoken": "^7.1.7",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.13.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-pure-render": "^1.0.2",
    "react-redux": "^5.0.5",
    "react-router": "^2.5.0",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "shortid": "^2.2.6",
    "validator": "^5.4.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "eject": "react-scripts eject"
  }
}
